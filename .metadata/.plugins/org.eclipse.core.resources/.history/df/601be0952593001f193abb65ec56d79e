package br.com.tema2.animais;

public class Cachorro {

	private String raca;
	private String cor;
	private char sexo;
	private String porte;
	
	
	
	public String getRaca() {
		return raca;
	}
	public void setRaca(String raca) {
		this.raca = raca;
	}
	public String getCor() {
		return cor;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public char getSexo() {
		return sexo;
	}
	public void setSexo(char sexo) {
		this.sexo = sexo;
	}
	public String getPorte() {
		return porte;
	}
	public void setPorte(String porte) {
		this.porte = porte;
	}
	
	
	public boolean comer(int qtd) {
		
		if (qtd >= 5) {
			return true;
		}else if (qtd <5) {
			return false;
		}
		return false;
	}
	
	public void latir() {
		System.out.println("Au au!");
	}
	
	public void andar() {
		System.out.println("O cachorro estÃ¡ andando!");
	}
	
	public void brincar() {
		System.out.println("O cachorro estÃ¡ brincando!");
	}
	
}
